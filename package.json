{
  "name": "element-plus-nuxt-starter",
  "packageManager": "pnpm@8.12.1",
  "version": "0.1.0",
  "description": "Element Plus with Nuxt 3 Minimal Starter",
  "repository": {
    "type": "git",
    "url": "git://github.com/AnotiaWang/element-plus-nuxt-starter"
  },
  "author": "Element Plus Team",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "nuxi dev",
    "build": "nuxi build",
    "preview": "nuxi preview",
    "start": "node .output/server/index.mjs",
    "generate": "nuxi generate",
    "postinstall": "nuxi prepare",
    "format": "prettier --write \"**/*.{js,ts,vue}\" --ignore-path .prettierignore"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.7.6"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.0.9",
    "@nuxtjs/color-mode": "^3.4.2",
    "@pinia/nuxt": "^0.5.1",
    "@unocss/nuxt": "^0.61.3",
    "@vueuse/nuxt": "^10.11.0",
    "nuxt": "^3.12.3",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "pinia": "^2.1.7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack",
        "vite",
        "vue"
      ]
    }
  }
}